<html> 
  <head> 
    <title>SVBR</title> 
  </head> 
  <body> 
    <script language="VBScript"> 
      Document.Write "1 + 1 = 2 is " & (1 + 1 = 2) 
      Document.Write "<BR/>" 
      Document.Write "10 - 6 * 3 = 12 is " & (10 - 6 * 3 = 12) 
      Document.Write "<BR/>"
	  Document.Write "<BR/>"
	  
	  
	  va = 3
      vb = 1
      Document.Write "not va is " & (Not va) & "</br>"
      Document.Write "va and vb is " & (va And vb) & "</br>"
      Document.Write "va xor vb is " & (va Xor vb) & "</br>"
      Document.Write "va or vb is " & (va Or vb) & "</br>"
      Document.Write "va eqv vb is " & (va Eqv vb) & "</br>"
      Document.Write "va imp vb is " & (va Imp vb) & "</br>"
    </script>
  </body>
</html>